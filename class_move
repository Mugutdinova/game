class c_move extends c0{
    f1(){
        if(time%40==0) this.alpha = Math.random()*360;
        this.вперед(1);
        if(this.x>window.innerWidth*0.9){
            this.alpha = -90;
            this.вперед(1);
        }
        if(this.y>window.innerHeight*0.8){
            this.alpha = 180;
            this.вперед(1);
        }
    }
}
